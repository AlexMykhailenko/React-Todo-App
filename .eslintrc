{
  "extends": "airbnb" ,
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": 0,
    "indent": ["error", 4,  { "SwitchCase": 1 }],
    "linebreak-style": 0
  }
}
